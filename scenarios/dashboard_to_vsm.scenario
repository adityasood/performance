# Scenario : Users visit dashboard -> check for pipeline history -> visit vsm pages
scenario do
  name 'pipeline edit page'
  count 4
  rampup 4
  duration 600
  loop do
    url '/pipelines'
    url '/tab/pipeline/history/%{pipeline}'
    url '/pipelines/value_stream_map/%{pipeline}/%{pipelinecount}'
  end
end
